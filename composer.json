{
    "name": "mileschou/traditional-chinese-lint",
    "description": "Traditional Chinese Lint for check words",
    "keywords": ["lint"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/MilesChou/traditional-chinese-lint/issues",
        "source": "https://github.com/MilesChou/traditional-chinese-lint"
    },
    "authors": [
        {
            "name": "Miles Chou",
            "email": "jangconan@gmail.com"
        },
        {
            "name": "Ban",
            "email": "jim515jim@gmail.com"
        },
        {
            "name": "Gson Liang",
            "email": "yuanyu90221@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "laravel-zero/framework": "^11.0.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["tclint"]
}
